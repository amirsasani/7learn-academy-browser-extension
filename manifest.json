{
    "name": "7learn Academy",
    "description": "some data about current course and unit",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["tabs"],
    "content_scripts": [
        {
          "matches": ["https://7learn.ac/*"],
          "js": ["content.js"]
        }
      ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/logo-16.png",
            "32": "/images/logo-32.png",
            "48": "/images/logo-48.png",
            "128": "/images/logo-128.png"
          }
    },
    "icons": {
        "16": "/images/logo-16.png",
        "32": "/images/logo-32.png",
        "48": "/images/logo-48.png",
        "128": "/images/logo-128.png"
      }
  }